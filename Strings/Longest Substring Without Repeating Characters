int longestSubstring(string s) {
        int n=s.size();
        unordered_map<char,int> m;
        int i=0,j=0,maxi=0;
        for(int j=0;j<n;j++){
            if(m.find(s[j])!=m.end()){
                if(m[s[j]]>=i) i=m[s[j]]+1;
            }
            m[s[j]]=j;
            maxi=max(maxi,j-i+1);
        }
        return maxi;
}
