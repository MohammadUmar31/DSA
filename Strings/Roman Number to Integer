int romanToInteger(string s) {
        unordered_map<char,int> m;
        m['I']=1;
        m['V']=5;
        m['X']=10;
        m['L']=50;
        m['C']=100;
        m['D']=500;
        m['M']=1000;
        int ans=0;
        int n=s.size();
        int prev=m[s[n-1]];
        ans += prev;
        for(int i=n-2;i>=0;i--){
            int cur=m[s[i]];
            if(cur>=prev) ans += cur;
            else ans -= cur;
            prev=cur;
        }
        return ans;
}
