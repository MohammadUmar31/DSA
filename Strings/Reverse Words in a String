string reverseWords(string text) {
        stringstream ss(text);
        string word;
        vector<string> words;
        while(ss>>word){
            words.push_back(word);
        }
        int n=words.size();
        string ans;
        for(int i=n-1;i>=0;i--){
            ans += words[i] + ' ';
        }
        ans.pop_back();
        return ans;
}
