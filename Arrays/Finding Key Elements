vector<int> findKeyElements(const vector<int>& arr, int T) {
        int n=arr.size();
        vector<int> ans;
        int sum=0;
        for(int i=n-2;i>=0;i--){
            sum = sum + arr[i+1];
            if(arr[i]-sum>T){
                ans.push_back(arr[i]);
            }
        }
        reverse(ans.begin(),ans.end());
        ans.push_back(arr[n-1]);
        return ans;
}
