int countSubarrays(vector<int>& arr, int target) {
        unordered_map<int,int> m;
        int count=0;
        int sum=0;
        int n=arr.size();
        for(int i=0;i<n;i++){
            sum += arr[i];
            if(sum==target) count++;
            count += m[sum-target];
            m[sum]++;
        }
        return count;
}
