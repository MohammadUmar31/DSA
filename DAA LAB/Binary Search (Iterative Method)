int binarySearch(int arr[], int n, int target){
    int low = 0, high = n - 1;
    while (low <= high) {
        int mid = (low + high) / 2;
        if (target == arr[mid]) 
            return mid;
        else if (target > arr[mid]) 
            low = mid + 1;
        else if (target < arr[mid])
            high = mid - 1;
    }
    return -1;
}
